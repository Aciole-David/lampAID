#Palette:

#color                   : #RGB
color david-red          : #e53319 ##
color david-blue         : #197fe5 ##
color david-green        : #19cc19 ##
color david-magenta      : #cc4ccc ##
color david-light-gray   : #999999 ##
color david-dark-gray    : #666666 ##
color david-yellow-print : #ffff00 ##

#Colormap listing - suitable for reloading.
#Character matching is case-sensitive.

[DAVID_SEAVIEW]
#MY VERSION OF david-derived colours for nucleotides
#symbols =>  color              #comment
.        =>  david-dark-gray    #wildcard/mismatch
?        ->  david-light-gray   #unknown
Aa       =>  david-red          #match
Cc       =>  david-green        #match
Gg       =>  david-yellow-print #match
Tt       =>  david-blue         #match
Uu       =>  david-magenta      #match

